<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/aws-ontology#"
     xml:base="http://www.semanticweb.org/aws-ontology"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:aws="http://www.semanticweb.org/aws-ontology#">
    
    <owl:Ontology rdf:about="http://www.semanticweb.org/aws-ontology">
        <rdfs:label>AWS Resource Ontology</rdfs:label>
        <rdfs:comment>An ontology for describing AWS resources, services, and their relationships.</rdfs:comment>
        <owl:versionInfo>0.1.0</owl:versionInfo>
    </owl:Ontology>
    
    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->
    
    <!-- Structural/Compositional Properties -->
    
    <owl:ObjectProperty rdf:about="#hasRegion">
        <rdfs:label>has region</rdfs:label>
        <rdfs:comment>Relates an AWS account to its regions</rdfs:comment>
        <rdfs:domain rdf:resource="#AWSAccount"/>
        <rdfs:range rdf:resource="#AWSRegion"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#hasAvailabilityZone">
        <rdfs:label>has availability zone</rdfs:label>
        <rdfs:comment>Relates a region to its availability zones</rdfs:comment>
        <rdfs:domain rdf:resource="#AWSRegion"/>
        <rdfs:range rdf:resource="#AvailabilityZone"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#contains">
        <rdfs:label>contains</rdfs:label>
        <rdfs:comment>General containment relationship</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#attachedTo">
        <rdfs:label>attached to</rdfs:label>
        <rdfs:comment>Relates resources that are attached to other resources</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#belongsToVPC">
        <rdfs:label>belongs to VPC</rdfs:label>
        <rdfs:comment>Relates resources to their VPC</rdfs:comment>
        <rdfs:range rdf:resource="#VPC"/>
    </owl:ObjectProperty>

    <!-- Access/Permission Properties -->

    <owl:ObjectProperty rdf:about="#canAssumeRole">
        <rdfs:label>can assume role</rdfs:label>
        <rdfs:comment>Indicates which roles can be assumed</rdfs:comment>
        <rdfs:range rdf:resource="#IAMRole"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#hasPolicy">
        <rdfs:label>has policy</rdfs:label>
        <rdfs:comment>Relates IAM entities to their policies</rdfs:comment>
        <rdfs:range rdf:resource="#IAMPolicy"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#appliesTo">
        <rdfs:label>applies to</rdfs:label>
        <rdfs:comment>Relates security groups to resources</rdfs:comment>
        <rdfs:domain rdf:resource="#EC2SecurityGroup"/>
    </owl:ObjectProperty>

    <!-- Operational/Functional Properties -->

    <owl:ObjectProperty rdf:about="#uses">
        <rdfs:label>uses</rdfs:label>
        <rdfs:comment>Indicates usage relationship between resources</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#manages">
        <rdfs:label>manages</rdfs:label>
        <rdfs:comment>Indicates management relationship between resources</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#monitors">
        <rdfs:label>monitors</rdfs:label>
        <rdfs:comment>Relates monitoring resources to their targets</rdfs:comment>
        <rdfs:domain rdf:resource="#CloudWatchAlarm"/>
        <rdfs:range rdf:resource="#CloudWatchMetric"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#logsActivityOf">
        <rdfs:label>logs activity of</rdfs:label>
        <rdfs:comment>Relates logging resources to their subjects</rdfs:comment>
        <rdfs:domain rdf:resource="#CloudTrailLog"/>
        <rdfs:range rdf:resource="#AWSAccount"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#routesTrafficThrough">
        <rdfs:label>routes traffic through</rdfs:label>
        <rdfs:comment>Indicates traffic routing relationships</rdfs:comment>
        <rdfs:domain rdf:resource="#RouteTable"/>
    </owl:ObjectProperty>

    <!-- Dependency Properties -->

    <owl:ObjectProperty rdf:about="#dependsOn">
        <rdfs:label>depends on</rdfs:label>
        <rdfs:comment>General dependency relationship between resources</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#isSourceFor">
        <rdfs:label>is source for</rdfs:label>
        <rdfs:comment>Indicates source relationship between resources</rdfs:comment>
    </owl:ObjectProperty>

    <!-- IAM-Specific Object Properties -->
    
    <owl:ObjectProperty rdf:about="#hasPolicyAttachment">
        <rdfs:label>has policy attachment</rdfs:label>
        <rdfs:comment>Relates IAM entities to their attached policies</rdfs:comment>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#IAMUser"/>
                    <rdf:Description rdf:about="#IAMRole"/>
                    <rdf:Description rdf:about="#IAMGroup"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="#IAMPolicy"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#hasPermissionBoundary">
        <rdfs:label>has permission boundary</rdfs:label>
        <rdfs:comment>Relates IAM entities to their permission boundary policies</rdfs:comment>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#IAMUser"/>
                    <rdf:Description rdf:about="#IAMRole"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="#IAMPermissionBoundary"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#hasTrustRelationship">
        <rdfs:label>has trust relationship</rdfs:label>
        <rdfs:comment>Relates IAM roles to their trust policies</rdfs:comment>
        <rdfs:domain rdf:resource="#IAMRole"/>
        <rdfs:range rdf:resource="#IAMTrustPolicy"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#memberOf">
        <rdfs:label>member of</rdfs:label>
        <rdfs:comment>Relates IAM users to their groups</rdfs:comment>
        <rdfs:domain rdf:resource="#IAMUser"/>
        <rdfs:range rdf:resource="#IAMGroup"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#controlledByServiceControlPolicy">
        <rdfs:label>controlled by service control policy</rdfs:label>
        <rdfs:comment>Relates AWS accounts to service control policies</rdfs:comment>
        <rdfs:domain rdf:resource="#AWSAccount"/>
        <rdfs:range rdf:resource="#ServiceControlPolicy"/>
    </owl:ObjectProperty>

    <!-- IAM-Specific Data Properties -->

    <owl:DatatypeProperty rdf:about="#policyDocument">
        <rdfs:label>policy document</rdfs:label>
        <rdfs:comment>The JSON policy document content</rdfs:comment>
        <rdfs:domain rdf:resource="#IAMPolicy"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#maxSessionDuration">
        <rdfs:label>max session duration</rdfs:label>
        <rdfs:comment>Maximum session duration in seconds</rdfs:comment>
        <rdfs:domain rdf:resource="#IAMRole"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    </owl:DatatypeProperty>

    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <!-- Infrastructure Classes -->

    <owl:Class rdf:about="#AWSAccount">
        <rdfs:label>AWS Account</rdfs:label>
        <rdfs:comment>An AWS account that can contain multiple services and resources</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="#AWSRegion">
        <rdfs:label>AWS Region</rdfs:label>
        <rdfs:comment>A geographical region containing AWS resources</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="#AvailabilityZone">
        <rdfs:label>Availability Zone</rdfs:label>
        <rdfs:comment>An isolated location within a region</rdfs:comment>
    </owl:Class>

    <!-- Compute Classes -->

    <owl:Class rdf:about="#EC2Instance">
        <rdfs:label>EC2 Instance</rdfs:label>
        <rdfs:comment>An Amazon EC2 virtual machine instance</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#ComputeResource"/>
    </owl:Class>

    <owl:Class rdf:about="#EC2AMI">
        <rdfs:label>EC2 AMI</rdfs:label>
        <rdfs:comment>An Amazon Machine Image for EC2 instances</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#ComputeResource"/>
    </owl:Class>

    <owl:Class rdf:about="#EC2SecurityGroup">
        <rdfs:label>EC2 Security Group</rdfs:label>
        <rdfs:comment>A security group controlling EC2 instance access</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#SecurityResource"/>
    </owl:Class>

    <owl:Class rdf:about="#EC2LoadBalancer">
        <rdfs:label>EC2 Load Balancer</rdfs:label>
        <rdfs:comment>A load balancer distributing traffic to EC2 instances</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#NetworkingResource"/>
    </owl:Class>

    <!-- Storage Classes -->

    <owl:Class rdf:about="#S3Bucket">
        <rdfs:label>S3 Bucket</rdfs:label>
        <rdfs:comment>An Amazon S3 storage bucket</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#StorageResource"/>
    </owl:Class>

    <owl:Class rdf:about="#S3Object">
        <rdfs:label>S3 Object</rdfs:label>
        <rdfs:comment>An object stored in an S3 bucket</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#StorageResource"/>
    </owl:Class>

    <owl:Class rdf:about="#EBSVolume">
        <rdfs:label>EBS Volume</rdfs:label>
        <rdfs:comment>An Elastic Block Store volume</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#StorageResource"/>
    </owl:Class>

    <owl:Class rdf:about="#EBSSnapshot">
        <rdfs:label>EBS Snapshot</rdfs:label>
        <rdfs:comment>A snapshot of an EBS volume</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#StorageResource"/>
    </owl:Class>

    <!-- Database Classes -->

    <owl:Class rdf:about="#RDSInstance">
        <rdfs:label>RDS Instance</rdfs:label>
        <rdfs:comment>An Amazon RDS database instance</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#DatabaseResource"/>
    </owl:Class>

    <owl:Class rdf:about="#RDSDatabase">
        <rdfs:label>RDS Database</rdfs:label>
        <rdfs:comment>A database within an RDS instance</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#DatabaseResource"/>
    </owl:Class>

    <!-- Identity Classes -->

    <owl:Class rdf:about="#IAMUser">
        <rdfs:label>IAM User</rdfs:label>
        <rdfs:comment>An IAM user entity</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#IdentityResource"/>
    </owl:Class>

    <owl:Class rdf:about="#IAMRole">
        <rdfs:label>IAM Role</rdfs:label>
        <rdfs:comment>An IAM role entity</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#IdentityResource"/>
    </owl:Class>

    <owl:Class rdf:about="#IAMPolicy">
        <rdfs:label>IAM Policy</rdfs:label>
        <rdfs:comment>Base class for all IAM policies</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#IdentityResource"/>
    </owl:Class>

    <owl:Class rdf:about="#IAMGroup">
        <rdfs:label>IAM Group</rdfs:label>
        <rdfs:comment>An IAM user group</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#IdentityResource"/>
    </owl:Class>

    <!-- Extended IAM Classes -->

    <owl:Class rdf:about="#ManagedPolicy">
        <rdfs:label>Managed Policy</rdfs:label>
        <rdfs:comment>An IAM managed policy (AWS or Customer managed)</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#IAMPolicy"/>
    </owl:Class>

    <owl:Class rdf:about="#AWSManagedPolicy">
        <rdfs:label>AWS Managed Policy</rdfs:label>
        <rdfs:comment>An AWS-provided managed policy</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#ManagedPolicy"/>
    </owl:Class>

    <owl:Class rdf:about="#CustomerManagedPolicy">
        <rdfs:label>Customer Managed Policy</rdfs:label>
        <rdfs:comment>A customer-created managed policy</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#ManagedPolicy"/>
    </owl:Class>

    <owl:Class rdf:about="#InlinePolicy">
        <rdfs:label>Inline Policy</rdfs:label>
        <rdfs:comment>An inline policy embedded in an IAM entity</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#IAMPolicy"/>
    </owl:Class>

    <owl:Class rdf:about="#IAMPermissionBoundary">
        <rdfs:label>Permission Boundary</rdfs:label>
        <rdfs:comment>A policy used as a permissions boundary</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#IAMPolicy"/>
    </owl:Class>

    <owl:Class rdf:about="#IAMTrustPolicy">
        <rdfs:label>Trust Policy</rdfs:label>
        <rdfs:comment>A policy defining which principals can assume a role</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#IAMPolicy"/>
    </owl:Class>

    <owl:Class rdf:about="#ServiceControlPolicy">
        <rdfs:label>Service Control Policy</rdfs:label>
        <rdfs:comment>An AWS Organizations service control policy</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#IAMPolicy"/>
    </owl:Class>

    <owl:Class rdf:about="#PolicyStatement">
        <rdfs:label>Policy Statement</rdfs:label>
        <rdfs:comment>A statement within an IAM policy</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#IdentityResource"/>
    </owl:Class>

    <!-- Policy Statement Components -->
    
    <owl:Class rdf:about="#Effect">
        <rdfs:label>Effect</rdfs:label>
        <rdfs:comment>The effect of a policy statement (Allow/Deny)</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="#Action">
        <rdfs:label>Action</rdfs:label>
        <rdfs:comment>An AWS API action</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="#Resource">
        <rdfs:label>Resource</rdfs:label>
        <rdfs:comment>An AWS resource ARN</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="#Condition">
        <rdfs:label>Condition</rdfs:label>
        <rdfs:comment>A condition for policy evaluation</rdfs:comment>
    </owl:Class>

    <!-- Networking Classes -->

    <owl:Class rdf:about="#VPC">
        <rdfs:label>VPC</rdfs:label>
        <rdfs:comment>A Virtual Private Cloud</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#NetworkingResource"/>
    </owl:Class>

    <owl:Class rdf:about="#Subnet">
        <rdfs:label>Subnet</rdfs:label>
        <rdfs:comment>A subnet within a VPC</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#NetworkingResource"/>
    </owl:Class>

    <owl:Class rdf:about="#RouteTable">
        <rdfs:label>Route Table</rdfs:label>
        <rdfs:comment>A VPC route table</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#NetworkingResource"/>
    </owl:Class>

    <owl:Class rdf:about="#InternetGateway">
        <rdfs:label>Internet Gateway</rdfs:label>
        <rdfs:comment>A VPC internet gateway</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#NetworkingResource"/>
    </owl:Class>

    <owl:Class rdf:about="#NATGateway">
        <rdfs:label>NAT Gateway</rdfs:label>
        <rdfs:comment>A VPC NAT gateway</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#NetworkingResource"/>
    </owl:Class>

    <!-- Monitoring Classes -->

    <owl:Class rdf:about="#CloudWatchMetric">
        <rdfs:label>CloudWatch Metric</rdfs:label>
        <rdfs:comment>A CloudWatch metric</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#MonitoringResource"/>
    </owl:Class>

    <owl:Class rdf:about="#CloudWatchAlarm">
        <rdfs:label>CloudWatch Alarm</rdfs:label>
        <rdfs:comment>A CloudWatch alarm</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#MonitoringResource"/>
    </owl:Class>

    <owl:Class rdf:about="#CloudTrailLog">
        <rdfs:label>CloudTrail Log</rdfs:label>
        <rdfs:comment>A CloudTrail log entry</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#MonitoringResource"/>
    </owl:Class>

    <owl:Class rdf:about="#LambdaFunction">
        <rdfs:label>Lambda Function</rdfs:label>
        <rdfs:comment>An AWS Lambda function</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#ComputeResource"/>
    </owl:Class>

    <!-- Base Resource Classes -->

    <owl:Class rdf:about="#AWSResource">
        <rdfs:label>AWS Resource</rdfs:label>
        <rdfs:comment>Base class for all AWS resources</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="#ComputeResource">
        <rdfs:label>Compute Resource</rdfs:label>
        <rdfs:comment>Base class for compute resources</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#AWSResource"/>
    </owl:Class>

    <owl:Class rdf:about="#StorageResource">
        <rdfs:label>Storage Resource</rdfs:label>
        <rdfs:comment>Base class for storage resources</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#AWSResource"/>
    </owl:Class>

    <owl:Class rdf:about="#NetworkingResource">
        <rdfs:label>Networking Resource</rdfs:label>
        <rdfs:comment>Base class for networking resources</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#AWSResource"/>
    </owl:Class>

    <owl:Class rdf:about="#SecurityResource">
        <rdfs:label>Security Resource</rdfs:label>
        <rdfs:comment>Base class for security resources</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#AWSResource"/>
    </owl:Class>

    <owl:Class rdf:about="#IdentityResource">
        <rdfs:label>Identity Resource</rdfs:label>
        <rdfs:comment>Base class for identity and access management resources</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#AWSResource"/>
    </owl:Class>

    <owl:Class rdf:about="#DatabaseResource">
        <rdfs:label>Database Resource</rdfs:label>
        <rdfs:comment>Base class for database resources</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#AWSResource"/>
    </owl:Class>

    <owl:Class rdf:about="#MonitoringResource">
        <rdfs:label>Monitoring Resource</rdfs:label>
        <rdfs:comment>Base class for monitoring and logging resources</rdfs:comment>
        <rdfs:subClassOf rdf:resource="#AWSResource"/>
    </owl:Class>

</rdf:RDF> 