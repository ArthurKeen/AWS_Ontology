    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Semantic Axioms: Disjoint Classes and Additional Constraints
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <!-- Disjoint Base Resource Classes -->
    <owl:AllDisjointClasses>
        <owl:members rdf:parseType="Collection">
            <rdf:Description rdf:about="#ComputeResource"/>
            <rdf:Description rdf:about="#StorageResource"/>
            <rdf:Description rdf:about="#NetworkingResource"/>
            <rdf:Description rdf:about="#SecurityResource"/>
            <rdf:Description rdf:about="#IdentityResource"/>
            <rdf:Description rdf:about="#DatabaseResource"/>
            <rdf:Description rdf:about="#MonitoringResource"/>
            <rdf:Description rdf:about="#IntegrationResource"/>
        </owl:members>
    </owl:AllDisjointClasses>

    <!-- Disjoint IAM Policy Types -->
    <owl:AllDisjointClasses>
        <owl:members rdf:parseType="Collection">
            <rdf:Description rdf:about="#ManagedPolicy"/>
            <rdf:Description rdf:about="#InlinePolicy"/>
        </owl:members>
    </owl:AllDisjointClasses>

    <!-- Disjoint Managed Policy Types -->
    <owl:AllDisjointClasses>
        <owl:members rdf:parseType="Collection">
            <rdf:Description rdf:about="#AWSManagedPolicy"/>
            <rdf:Description rdf:about="#CustomerManagedPolicy"/>
        </owl:members>
    </owl:AllDisjointClasses>

    <!-- Disjoint Container Service Types -->
    <owl:AllDisjointClasses>
        <owl:members rdf:parseType="Collection">
            <rdf:Description rdf:about="#ECSCluster"/>
            <rdf:Description rdf:about="#EKSCluster"/>
            <rdf:Description rdf:about="#FargateService"/>
        </owl:members>
    </owl:AllDisjointClasses>

    <!-- Enhanced Cardinality Constraints -->
    
    <!-- API Gateway must have at least one stage -->
    <owl:Class rdf:about="#APIGateway">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasStage"/>
                <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <!-- ECS Service must have exactly one task definition -->
    <owl:Class rdf:about="#ECSService">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasTaskDefinition"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <!-- ECS Service must run on exactly one cluster -->
    <owl:Class rdf:about="#ECSService">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#runsOnCluster"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <!-- Step Function must have at least one activity or state -->
    <owl:Class rdf:about="#StepFunction">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasActivity"/>
                <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <!-- EventBridge Rule must have at least one target -->
    <owl:Class rdf:about="#EventBridgeRule">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasTarget"/>
                <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <!-- IAM User can belong to multiple groups -->
    <owl:Class rdf:about="#IAMUser">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#memberOf"/>
                <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">0</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <!-- VPC must be in exactly one region -->
    <owl:Class rdf:about="#VPC">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#belongsToRegion"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <!-- Container Image must be stored in exactly one repository -->
    <owl:Class rdf:about="#ContainerImage">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#storedInRepository"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <!-- Functional Properties (can have at most one value) -->
    
    <!-- Each resource has exactly one ARN -->
    <owl:FunctionalProperty rdf:about="#arn"/>
    
    <!-- Each resource has exactly one creation date -->
    <owl:FunctionalProperty rdf:about="#creationDate"/>
    
    <!-- Each API Gateway has exactly one type -->
    <owl:FunctionalProperty rdf:about="#apiGatewayType"/>
    
    <!-- Each Step Function has exactly one type -->
    <owl:FunctionalProperty rdf:about="#stateMachineType"/>
    
    <!-- Each SQS Queue has exactly one type -->
    <owl:FunctionalProperty rdf:about="#queueType"/>

    <!-- Inverse Functional Properties (at most one subject can have this relationship to any object) -->
    
    <!-- Each ARN uniquely identifies one resource -->
    <owl:InverseFunctionalProperty rdf:about="#arn"/>

    <!-- Symmetric Properties -->
    
    <!-- Resource sharing is symmetric -->
    <owl:SymmetricProperty rdf:about="#sharesResourcesWith"/>

    <!-- Transitive Properties -->
    
    <!-- Control relationships are transitive -->
    <owl:TransitiveProperty rdf:about="#controls"/>
    
    <!-- Dependency relationships are transitive -->
    <owl:TransitiveProperty rdf:about="#dependsOn"/>
    
    <!-- Temporal ordering is transitive -->
    <owl:TransitiveProperty rdf:about="#createdBefore"/>
    
    <!-- Succession is transitive -->
    <owl:TransitiveProperty rdf:about="#succeeds"/>

</rdf:RDF> 