<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/aws-ontology#"
     xml:base="http://www.semanticweb.org/aws-ontology"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:aws="http://www.semanticweb.org/aws-ontology#">
    
    <owl:Ontology rdf:about="http://www.semanticweb.org/aws-ontology">
        <rdfs:label>AWS Resource Ontology</rdfs:label>
        <rdfs:comment>An ontology for describing AWS resources, services, and their relationships.</rdfs:comment>
        <owl:versionInfo>0.2.0</owl:versionInfo>
    </owl:Ontology>
    
    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->
    
    <!-- Infrastructure Object Properties -->
    <owl:ObjectProperty rdf:about="#hasRegion">
        <rdfs:label>has region</rdfs:label>
        <rdfs:comment>Relates an AWS account to its regions</rdfs:comment>
        <rdfs:domain rdf:resource="#AWSAccount"/>
        <rdfs:range rdf:resource="#AWSRegion"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#regionOf">
        <rdfs:label>region of</rdfs:label>
        <rdfs:comment>Inverse of has region</rdfs:comment>
        <owl:inverseOf rdf:resource="#hasRegion"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#hasAvailabilityZone">
        <rdfs:label>has availability zone</rdfs:label>
        <rdfs:comment>Relates a region to its availability zones</rdfs:comment>
        <rdfs:domain rdf:resource="#AWSRegion"/>
        <rdfs:range rdf:resource="#AvailabilityZone"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#availabilityZoneOf">
        <rdfs:label>availability zone of</rdfs:label>
        <rdfs:comment>Inverse of has availability zone</rdfs:comment>
        <owl:inverseOf rdf:resource="#hasAvailabilityZone"/>
    </owl:ObjectProperty>

    <!-- Structural/Compositional Properties -->
    <owl:ObjectProperty rdf:about="#contains">
        <rdfs:label>contains</rdfs:label>
        <rdfs:comment>General containment relationship</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#containedBy">
        <rdfs:label>contained by</rdfs:label>
        <rdfs:comment>Inverse of contains</rdfs:comment>
        <owl:inverseOf rdf:resource="#contains"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#attachedTo">
        <rdfs:label>attached to</rdfs:label>
        <rdfs:comment>Relates resources that are attached to other resources</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#attachmentOf">
        <rdfs:label>attachment of</rdfs:label>
        <rdfs:comment>Inverse of attached to</rdfs:comment>
        <owl:inverseOf rdf:resource="#attachedTo"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#belongsToVPC">
        <rdfs:label>belongs to VPC</rdfs:label>
        <rdfs:comment>Relates resources to their VPC</rdfs:comment>
        <rdfs:range rdf:resource="#VPC"/>
    </owl:ObjectProperty>

    <!-- IAM-Specific Object Properties -->
    <owl:ObjectProperty rdf:about="#hasPolicyAttachment">
        <rdfs:label>has policy attachment</rdfs:label>
        <rdfs:comment>Relates IAM entities to their attached policies</rdfs:comment>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#IAMUser"/>
                    <rdf:Description rdf:about="#IAMRole"/>
                    <rdf:Description rdf:about="#IAMGroup"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="#IAMPolicy"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#hasPermissionBoundary">
        <rdfs:label>has permission boundary</rdfs:label>
        <rdfs:comment>Relates IAM entities to their permission boundary policies</rdfs:comment>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#IAMUser"/>
                    <rdf:Description rdf:about="#IAMRole"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="#IAMPermissionBoundary"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#hasTrustRelationship">
        <rdfs:label>has trust relationship</rdfs:label>
        <rdfs:comment>Relates IAM roles to their trust policies</rdfs:comment>
        <rdfs:domain rdf:resource="#IAMRole"/>
        <rdfs:range rdf:resource="#IAMTrustPolicy"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#memberOf">
        <rdfs:label>member of</rdfs:label>
        <rdfs:comment>Relates IAM users to their groups</rdfs:comment>
        <rdfs:domain rdf:resource="#IAMUser"/>
        <rdfs:range rdf:resource="#IAMGroup"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#hasMember">
        <rdfs:label>has member</rdfs:label>
        <rdfs:comment>Inverse of member of</rdfs:comment>
        <owl:inverseOf rdf:resource="#memberOf"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#controlledByServiceControlPolicy">
        <rdfs:label>controlled by service control policy</rdfs:label>
        <rdfs:comment>Relates AWS accounts to service control policies</rdfs:comment>
        <rdfs:domain rdf:resource="#AWSAccount"/>
        <rdfs:range rdf:resource="#ServiceControlPolicy"/>
    </owl:ObjectProperty>

    <!-- Operational/Functional Properties -->
    <owl:ObjectProperty rdf:about="#uses">
        <rdfs:label>uses</rdfs:label>
        <rdfs:comment>Indicates usage relationship between resources</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#usedBy">
        <rdfs:label>used by</rdfs:label>
        <rdfs:comment>Inverse of uses</rdfs:comment>
        <owl:inverseOf rdf:resource="#uses"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#manages">
        <rdfs:label>manages</rdfs:label>
        <rdfs:comment>Indicates management relationship between resources</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#managedBy">
        <rdfs:label>managed by</rdfs:label>
        <rdfs:comment>Inverse of manages</rdfs:comment>
        <owl:inverseOf rdf:resource="#manages"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#monitors">
        <rdfs:label>monitors</rdfs:label>
        <rdfs:comment>Relates monitoring resources to their targets</rdfs:comment>
        <rdfs:domain rdf:resource="#CloudWatchAlarm"/>
        <rdfs:range rdf:resource="#CloudWatchMetric"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#monitoredBy">
        <rdfs:label>monitored by</rdfs:label>
        <rdfs:comment>Inverse of monitors</rdfs:comment>
        <owl:inverseOf rdf:resource="#monitors"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#logsActivityOf">
        <rdfs:label>logs activity of</rdfs:label>
        <rdfs:comment>Relates logging resources to their subjects</rdfs:comment>
        <rdfs:domain rdf:resource="#CloudTrailLog"/>
        <rdfs:range rdf:resource="#AWSAccount"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#routesTrafficThrough">
        <rdfs:label>routes traffic through</rdfs:label>
        <rdfs:comment>Indicates traffic routing relationships</rdfs:comment>
        <rdfs:domain rdf:resource="#RouteTable"/>
    </owl:ObjectProperty>

    <!-- Dependency Properties -->
    <owl:ObjectProperty rdf:about="#dependsOn">
        <rdfs:label>depends on</rdfs:label>
        <rdfs:comment>General dependency relationship between resources</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#dependencyOf">
        <rdfs:label>dependency of</rdfs:label>
        <rdfs:comment>Inverse of depends on</rdfs:comment>
        <owl:inverseOf rdf:resource="#dependsOn"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#isSourceFor">
        <rdfs:label>is source for</rdfs:label>
        <rdfs:comment>Indicates source relationship between resources</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#hasSource">
        <rdfs:label>has source</rdfs:label>
        <rdfs:comment>Inverse of is source for</rdfs:comment>
        <owl:inverseOf rdf:resource="#isSourceFor"/>
    </owl:ObjectProperty>

    <!-- Policy Statement Properties -->
    <owl:ObjectProperty rdf:about="#hasStatement">
        <rdfs:label>has statement</rdfs:label>
        <rdfs:comment>Relates policies to their statements</rdfs:comment>
        <rdfs:domain rdf:resource="#IAMPolicy"/>
        <rdfs:range rdf:resource="#PolicyStatement"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#hasEffect">
        <rdfs:label>has effect</rdfs:label>
        <rdfs:comment>Relates policy statements to their effects</rdfs:comment>
        <rdfs:domain rdf:resource="#PolicyStatement"/>
        <rdfs:range rdf:resource="#Effect"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#hasAction">
        <rdfs:label>has action</rdfs:label>
        <rdfs:comment>Relates policy statements to their actions</rdfs:comment>
        <rdfs:domain rdf:resource="#PolicyStatement"/>
        <rdfs:range rdf:resource="#Action"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#hasResource">
        <rdfs:label>has resource</rdfs:label>
        <rdfs:comment>Relates policy statements to their target resources</rdfs:comment>
        <rdfs:domain rdf:resource="#PolicyStatement"/>
        <rdfs:range rdf:resource="#Resource"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#hasCondition">
        <rdfs:label>has condition</rdfs:label>
        <rdfs:comment>Relates policy statements to their conditions</rdfs:comment>
        <rdfs:domain rdf:resource="#PolicyStatement"/>
        <rdfs:range rdf:resource="#Condition"/>
    </owl:ObjectProperty>

    <!-- Container-Specific Object Properties -->
    <owl:ObjectProperty rdf:about="#runsOnCluster">
        <rdfs:label>runs on cluster</rdfs:label>
        <rdfs:comment>Relates container services/tasks to their cluster</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#clusterHosts">
        <rdfs:label>cluster hosts</rdfs:label>
        <rdfs:comment>Inverse of runs on cluster</rdfs:comment>
        <owl:inverseOf rdf:resource="#runsOnCluster"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#hasTaskDefinition">
        <rdfs:label>has task definition</rdfs:label>
        <rdfs:comment>Relates ECS services/tasks to their task definition</rdfs:comment>
        <rdfs:domain rdf:resource="#ECSService"/>
        <rdfs:range rdf:resource="#ECSTaskDefinition"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#taskDefinitionOf">
        <rdfs:label>task definition of</rdfs:label>
        <rdfs:comment>Inverse of has task definition</rdfs:comment>
        <owl:inverseOf rdf:resource="#hasTaskDefinition"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#usesContainerImage">
        <rdfs:label>uses container image</rdfs:label>
        <rdfs:comment>Relates container services to their container images</rdfs:comment>
        <rdfs:range rdf:resource="#ContainerImage"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#containerImageUsedBy">
        <rdfs:label>container image used by</rdfs:label>
        <rdfs:comment>Inverse of uses container image</rdfs:comment>
        <owl:inverseOf rdf:resource="#usesContainerImage"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#storedInRepository">
        <rdfs:label>stored in repository</rdfs:label>
        <rdfs:comment>Relates container images to their ECR repository</rdfs:comment>
        <rdfs:domain rdf:resource="#ContainerImage"/>
        <rdfs:range rdf:resource="#ECRRepository"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#repositoryContains">
        <rdfs:label>repository contains</rdfs:label>
        <rdfs:comment>Inverse of stored in repository</rdfs:comment>
        <owl:inverseOf rdf:resource="#storedInRepository"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#hasNodeGroup">
        <rdfs:label>has node group</rdfs:label>
        <rdfs:comment>Relates EKS clusters to their node groups</rdfs:comment>
        <rdfs:domain rdf:resource="#EKSCluster"/>
        <rdfs:range rdf:resource="#EKSNodeGroup"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#nodeGroupOf">
        <rdfs:label>node group of</rdfs:label>
        <rdfs:comment>Inverse of has node group</rdfs:comment>
        <owl:inverseOf rdf:resource="#hasNodeGroup"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#runsPod">
        <rdfs:label>runs pod</rdfs:label>
        <rdfs:comment>Relates EKS node groups or clusters to their pods</rdfs:comment>
        <rdfs:range rdf:resource="#EKSPod"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#podRunsOn">
        <rdfs:label>pod runs on</rdfs:label>
        <rdfs:comment>Inverse of runs pod</rdfs:comment>
        <owl:inverseOf rdf:resource="#runsPod"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#usesFargate">
        <rdfs:label>uses Fargate</rdfs:label>
        <rdfs:comment>Indicates that a service uses AWS Fargate for serverless containers</rdfs:comment>
        <rdfs:domain rdf:resource="#ECSService"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#appliesTo">
        <rdfs:label>applies to</rdfs:label>
        <rdfs:comment>Indicates that a policy or security group applies to a resource</rdfs:comment>
    </owl:ObjectProperty>

    <!-- API & Integration Object Properties -->
    <owl:ObjectProperty rdf:about="#hasStage">
        <rdfs:label>has stage</rdfs:label>
        <rdfs:comment>Relates an API Gateway to its deployment stages</rdfs:comment>
        <rdfs:domain rdf:resource="#APIGateway"/>
        <rdfs:range rdf:resource="#APIGatewayStage"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#stageOf">
        <rdfs:label>stage of</rdfs:label>
        <rdfs:comment>Inverse of has stage</rdfs:comment>
        <owl:inverseOf rdf:resource="#hasStage"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#hasResource">
        <rdfs:label>has resource</rdfs:label>
        <rdfs:comment>Relates an API Gateway to its resources</rdfs:comment>
        <rdfs:domain rdf:resource="#APIGateway"/>
        <rdfs:range rdf:resource="#APIGatewayResource"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#resourceOf">
        <rdfs:label>resource of</rdfs:label>
        <rdfs:comment>Inverse of has resource</rdfs:comment>
        <owl:inverseOf rdf:resource="#hasResource"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#hasMethod">
        <rdfs:label>has method</rdfs:label>
        <rdfs:comment>Relates an API Gateway resource to its HTTP methods</rdfs:comment>
        <rdfs:domain rdf:resource="#APIGatewayResource"/>
        <rdfs:range rdf:resource="#APIGatewayMethod"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#methodOf">
        <rdfs:label>method of</rdfs:label>
        <rdfs:comment>Inverse of has method</rdfs:comment>
        <owl:inverseOf rdf:resource="#hasMethod"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#integrationWith">
        <rdfs:label>integration with</rdfs:label>
        <rdfs:comment>Indicates integration between API Gateway methods and backend services</rdfs:comment>
        <rdfs:domain rdf:resource="#APIGatewayMethod"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#triggersStepFunction">
        <rdfs:label>triggers step function</rdfs:label>
        <rdfs:comment>Indicates that a service triggers a Step Function execution</rdfs:comment>
        <rdfs:range rdf:resource="#StepFunction"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#stepFunctionTriggeredBy">
        <rdfs:label>step function triggered by</rdfs:label>
        <rdfs:comment>Inverse of triggers step function</rdfs:comment>
        <owl:inverseOf rdf:resource="#triggersStepFunction"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#hasActivity">
        <rdfs:label>has activity</rdfs:label>
        <rdfs:comment>Relates a Step Function to its activities</rdfs:comment>
        <rdfs:domain rdf:resource="#StepFunction"/>
        <rdfs:range rdf:resource="#StepFunctionActivity"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#activityOf">
        <rdfs:label>activity of</rdfs:label>
        <rdfs:comment>Inverse of has activity</rdfs:comment>
        <owl:inverseOf rdf:resource="#hasActivity"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#publishesToTopic">
        <rdfs:label>publishes to topic</rdfs:label>
        <rdfs:comment>Indicates that a service publishes messages to an SNS topic</rdfs:comment>
        <rdfs:range rdf:resource="#SNSTopic"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#topicReceivesFrom">
        <rdfs:label>topic receives from</rdfs:label>
        <rdfs:comment>Inverse of publishes to topic</rdfs:comment>
        <owl:inverseOf rdf:resource="#publishesToTopic"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#subscribesTo">
        <rdfs:label>subscribes to</rdfs:label>
        <rdfs:comment>Indicates that a service subscribes to an SNS topic or SQS queue</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#hasSubscriber">
        <rdfs:label>has subscriber</rdfs:label>
        <rdfs:comment>Inverse of subscribes to</rdfs:comment>
        <owl:inverseOf rdf:resource="#subscribesTo"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#sendsToQueue">
        <rdfs:label>sends to queue</rdfs:label>
        <rdfs:comment>Indicates that a service sends messages to an SQS queue</rdfs:comment>
        <rdfs:range rdf:resource="#SQSQueue"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#queueReceivesFrom">
        <rdfs:label>queue receives from</rdfs:label>
        <rdfs:comment>Inverse of sends to queue</rdfs:comment>
        <owl:inverseOf rdf:resource="#sendsToQueue"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#hasRule">
        <rdfs:label>has rule</rdfs:label>
        <rdfs:comment>Relates an EventBridge to its rules</rdfs:comment>
        <rdfs:domain rdf:resource="#EventBridge"/>
        <rdfs:range rdf:resource="#EventBridgeRule"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#ruleOf">
        <rdfs:label>rule of</rdfs:label>
        <rdfs:comment>Inverse of has rule</rdfs:comment>
        <owl:inverseOf rdf:resource="#hasRule"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#hasTarget">
        <rdfs:label>has target</rdfs:label>
        <rdfs:comment>Relates an EventBridge rule to its targets</rdfs:comment>
        <rdfs:domain rdf:resource="#EventBridgeRule"/>
        <rdfs:range rdf:resource="#EventBridgeTarget"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#targetOf">
        <rdfs:label>target of</rdfs:label>
        <rdfs:comment>Inverse of has target</rdfs:comment>
        <owl:inverseOf rdf:resource="#hasTarget"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#routesEventTo">
        <rdfs:label>routes event to</rdfs:label>
        <rdfs:comment>Indicates event routing from EventBridge to target services</rdfs:comment>
        <rdfs:domain rdf:resource="#EventBridge"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#receivesEventFrom">
        <rdfs:label>receives event from</rdfs:label>
        <rdfs:comment>Inverse of routes event to</rdfs:comment>
        <owl:inverseOf rdf:resource="#routesEventTo"/>
    </owl:ObjectProperty>

    <!-- Temporal Relationship Properties -->
    <owl:ObjectProperty rdf:about="#createdBefore">
        <rdfs:label>created before</rdfs:label>
        <rdfs:comment>Indicates temporal ordering of resource creation</rdfs:comment>
        <rdfs:domain rdf:resource="#AWSResource"/>
        <rdfs:range rdf:resource="#AWSResource"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#createdAfter">
        <rdfs:label>created after</rdfs:label>
        <rdfs:comment>Inverse of created before</rdfs:comment>
        <owl:inverseOf rdf:resource="#createdBefore"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#replacedBy">
        <rdfs:label>replaced by</rdfs:label>
        <rdfs:comment>Indicates that one resource was replaced by another</rdfs:comment>
        <rdfs:domain rdf:resource="#AWSResource"/>
        <rdfs:range rdf:resource="#AWSResource"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#replaces">
        <rdfs:label>replaces</rdfs:label>
        <rdfs:comment>Inverse of replaced by</rdfs:comment>
        <owl:inverseOf rdf:resource="#replacedBy"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#migratedFrom">
        <rdfs:label>migrated from</rdfs:label>
        <rdfs:comment>Indicates migration source for a resource</rdfs:comment>
        <rdfs:domain rdf:resource="#AWSResource"/>
        <rdfs:range rdf:resource="#AWSResource"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#migratedTo">
        <rdfs:label>migrated to</rdfs:label>
        <rdfs:comment>Inverse of migrated from</rdfs:comment>
        <owl:inverseOf rdf:resource="#migratedFrom"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#succeeds">
        <rdfs:label>succeeds</rdfs:label>
        <rdfs:comment>Indicates logical succession relationship between resources</rdfs:comment>
        <rdfs:domain rdf:resource="#AWSResource"/>
        <rdfs:range rdf:resource="#AWSResource"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#precedes">
        <rdfs:label>precedes</rdfs:label>
        <rdfs:comment>Inverse of succeeds</rdfs:comment>
        <owl:inverseOf rdf:resource="#succeeds"/>
    </owl:ObjectProperty>

    <!-- Cost Relationship Properties -->
    <owl:ObjectProperty rdf:about="#incursChargeFor">
        <rdfs:label>incurs charge for</rdfs:label>
        <rdfs:comment>Indicates that one resource incurs costs for another</rdfs:comment>
        <rdfs:domain rdf:resource="#AWSResource"/>
        <rdfs:range rdf:resource="#AWSResource"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#chargedBy">
        <rdfs:label>charged by</rdfs:label>
        <rdfs:comment>Inverse of incurs charge for</rdfs:comment>
        <owl:inverseOf rdf:resource="#incursChargeFor"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#optimizedBy">
        <rdfs:label>optimized by</rdfs:label>
        <rdfs:comment>Indicates cost or performance optimization relationships</rdfs:comment>
        <rdfs:domain rdf:resource="#AWSResource"/>
        <rdfs:range rdf:resource="#AWSResource"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#optimizes">
        <rdfs:label>optimizes</rdfs:label>
        <rdfs:comment>Inverse of optimized by</rdfs:comment>
        <owl:inverseOf rdf:resource="#optimizedBy"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#sharesResourcesWith">
        <rdfs:label>shares resources with</rdfs:label>
        <rdfs:comment>Indicates resource sharing for cost efficiency</rdfs:comment>
        <rdfs:domain rdf:resource="#AWSResource"/>
        <rdfs:range rdf:resource="#AWSResource"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#allocatesCostTo">
        <rdfs:label>allocates cost to</rdfs:label>
        <rdfs:comment>Indicates cost allocation relationships</rdfs:comment>
        <rdfs:domain rdf:resource="#AWSResource"/>
        <rdfs:range rdf:resource="#AWSResource"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#receivesCostFrom">
        <rdfs:label>receives cost from</rdfs:label>
        <rdfs:comment>Inverse of allocates cost to</rdfs:comment>
        <owl:inverseOf rdf:resource="#allocatesCostTo"/>
    </owl:ObjectProperty>

    <!-- Compliance Relationship Properties -->
    <owl:ObjectProperty rdf:about="#compliesWith">
        <rdfs:label>complies with</rdfs:label>
        <rdfs:comment>Indicates compliance with standards, regulations, or policies</rdfs:comment>
        <rdfs:domain rdf:resource="#AWSResource"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#enforces">
        <rdfs:label>enforces</rdfs:label>
        <rdfs:comment>Indicates that a resource enforces compliance rules</rdfs:comment>
        <rdfs:domain rdf:resource="#AWSResource"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#enforcedBy">
